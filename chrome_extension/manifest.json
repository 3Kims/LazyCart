{
  "manifest_version": 2,

  "name": "ez_basket_chromeExtension",
  "description": "easily add favorite product and compare price",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "login.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "https://www.naver.com/"
  ]
}